<% users = users_list -%>
<table>
  <tr>
    <th><%= I18n.t('users.name')%></th>
    <th><%= I18n.t('users.nick_name')%></th>
  </tr>
  <% users.each do |u| -%>
    <tr>
      <td><%= h u.name%></td><td><%= h u.nick_name%></td>
    </tr>
  <% end -%>
</table>
